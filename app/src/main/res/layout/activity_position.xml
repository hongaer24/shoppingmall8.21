<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".user.view.PositionActivity">

<LinearLayout
    android:id="@+id/ll_position"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp">

        <ImageButton
            android:id="@+id/ib_goods_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/top_bar_left_back" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="设置"
            android:textColor="#000000"
            android:textSize="20sp" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="8dp"
        android:background="#11000000" />

    <TextView
        android:id="@+id/tv_xinxi"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:drawablePadding="260dp"
        android:drawableRight="@drawable/home_arrow_right"
        android:padding="10dp"
        android:text="个人资料"
        android:textColor="#131212"
        android:textSize="17dp" />　

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="8dp"
        android:background="#11000000" />

    <TextView
        android:id="@+id/tv_IDsafe"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:layout_alignParentLeft="true"
        android:drawablePadding="260dp"
        android:drawableRight="@drawable/home_arrow_right"
        android:padding="12dp"
        android:text="账号安全"
        android:textColor="#000000"
        android:textSize="16dp" />　

    <View
        android:layout_width="match_parent"
        android:layout_height="8dp"
        android:layout_marginTop="10dp"
        android:background="#11000000" />
     <RelativeLayout
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:orientation="horizontal" >
        <TextView
        android:layout_width="wrap_content"
        android:layout_height="37dp"
        android:layout_alignParentLeft="true"
        android:padding="8dp"
        android:text="消息推送"
        android:textColor="#000000"
        android:textSize="17dp" />　

         <ToggleButton
             android:id="@+id/bt_push1"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_alignParentRight="true"
             android:background="@drawable/selector_butn_toggle"
             android:checked="true"
             android:text=""
             android:textOff=""
             android:textOn="" />

     </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="5dp"
        android:background="#11000000" />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_alignParentLeft="true"
            android:padding="10dp"
            android:text="订单推送"
            android:textColor="#000000"
            android:textSize="17dp" />　

        <ToggleButton
            android:id="@+id/bt_push"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:background="@drawable/selector_butn_toggle"
            android:checked="true"
            android:text=""
            android:textOff=""
            android:textOn="" />

    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:background="#11000000" />

    <TextView
        android:id="@+id/tv_flesh"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:drawablePadding="260dp"
        android:drawableRight="@drawable/home_arrow_right"
        android:padding="10dp"
        android:text="清除缓存"
        android:textColor="#000000"
        android:textSize="17dp" />　

    <View
        android:layout_width="match_parent"
        android:layout_height="8dp"
        android:layout_marginTop="10dp"
        android:background="#11000000" />

    <TextView
        android:id="@+id/tv_drawback"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:drawablePadding="260dp"
        android:drawableRight="@drawable/home_arrow_right"
        android:padding="10dp"
        android:text="意见反馈"
        android:textColor="#000000"
        android:textSize="17dp" />　

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:background="#11000000" />

    <TextView
        android:id="@+id/tv_shaerapp"
        android:layout_width="wrap_content"
        android:layout_height="39dp"
        android:layout_alignParentLeft="true"
        android:drawablePadding="285dp"
        android:drawableRight="@drawable/home_arrow_right"
        android:padding="9dp"
        android:text="分享app"
        android:textColor="#000000"
        android:textSize="17dp" />　

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:background="#11000000" />

    <TextView
        android:id="@+id/tv_for_us"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:drawablePadding="260dp"
        android:drawableRight="@drawable/home_arrow_right"
        android:padding="10dp"
        android:text="关于我们"
        android:textColor="#000000"
        android:textSize="17dp" />　

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:background="#11000000" />

    <TextView
        android:id="@+id/tv_check"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:drawablePadding="260dp"
        android:drawableRight="@drawable/home_arrow_right"
        android:padding="10dp"
        android:text="检查版本"
        android:textColor="#000000"
        android:textSize="17dp" />　

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_marginBottom="4dp"
        android:background="#11000000" />

    <Button
        android:id="@+id/bt_loingback"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="#4c44ed"
        android:text="退出登录"
        android:textColor="#ffffff" />

</LinearLayout>

</RelativeLayout>
